---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Eric Stein</title>
	<link href="https://steinea.ca/assets/xml/posts.xml/" rel="self"/>
	<link href="https://steinea.ca/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://steinea.ca/</id>
	<author>
		<name>Eric Stein</name>
		<email>steinea@protonmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://steinea.ca{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://steinea.ca{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
